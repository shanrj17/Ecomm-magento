<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Codazon_ThemeLayoutPro:etc/config.xsd">
    <default>
        <themelayoutpro>
            <env>
                <enable_dev_mode>0</enable_dev_mode>
                <minify_html>1</minify_html>
            </env>
            <store>
                <enable_ajax_captcha>1</enable_ajax_captcha>
            </store>
            <catalog_review>
                <enable_review_image>1</enable_review_image>
            </catalog_review>
		</themelayoutpro>
	</default>
</config>