<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <remove src="Magefan_Blog::css/blog-m.css" />
        <remove src="Magefan_Blog::css/blog-custom.css" />
    </head>
    <referenceBlock name="head.additional">
        <block name="home_head_config" class="Codazon\ThemeLayoutPro\Block\Header" template="Codazon_ThemeLayoutPro::layout/home-head-config.phtml" before="-" />
    </referenceBlock>
    <body>
        <referenceContainer name="main.content" htmlClass="page-main page-main-full" htmlTag="main" />
        <referenceContainer name="content">
            <block name="theme-main-content" class="Codazon\ThemeLayoutPro\Block\MainContent" />
        </referenceContainer>
    </body>
</page>